<?xml version="1.0"?>
<!-- this is an experimental script for testing the curent configuration with data from command readings (/mobile_base/commands/velocity) -->
<launch>
  <param name="use_sim_time" value="true" type="boolean"/> 
  <!-- robot localization filter (ukf) -->
  <node name="ekf2" pkg="robot_localization" type="ukf_localization_node" output="screen" >
    <rosparam command="load" file="$(find sensor_fusion)/param/robot_localization_params_secondary.yaml" />
    <remap from="/odometry/filtered" to="ekf2"/>
    <remap from="/cmd_vel" to="/cmd_vel_mux/input/teleop"/>
  </node>
  
  <!-- rviz -->
  <!-- <node name="rviz" pkg="rviz" type="rviz" args="-d $(find sensor_fusion)/rviz/odom.rviz"/> -->
  
  <node name="pose_conv2" pkg="sensor_fusion" type="pose_conversion_experiment.py" output="screen" />
  
  <node name="path_buliding2" pkg="sensor_fusion" type="path_building_experiment.py" output="screen" />
  
  
  
</launch>
